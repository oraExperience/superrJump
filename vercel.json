
{
  "version": 2,
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "api/index.js": {
      "memory": 1024,
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/(.*\\.(html|png|jpg|jpeg|gif|css|js|ico|svg|webp))",
      "dest": "/$1"
    },
    {
      "src": "/",
      "dest": "/home.html"
    }
  ]
}
